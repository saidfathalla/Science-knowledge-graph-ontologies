###  https://w3id.org/skgo/modsci#isApplicationOfSocialSciences
:isApplicationOfSocialSciences rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf :isApplicationOfScience ;
                               rdfs:comment "An application is an application of social science."@en ;
                               rdfs:label "is application of social science"@en .


###  https://w3id.org/skgo/modsci#isDiscoveredBy
:isDiscoveredBy rdf:type owl:ObjectProperty ,
                         owl:IrreflexiveProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( <http://sweetontology.net/phen/Phenomenon>
                                            :ScientificDiscovery
                                          )
                            ] ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( :ScientificOrganization
                                           :Scientist
                                         )
                           ] ;
                rdfs:comment "A phenomenon or a scientific discovery is discovered by either a scientist or an organization."@en ;
                rdfs:label "is discovered by"@en .


###  https://w3id.org/skgo/modsci#isDiscoveredByOrganization
:isDiscoveredByOrganization rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :isDiscoveredBy ;
                            rdfs:comment "A phenomenon or a scientific discovery is discovered by an organization."@en ;
                            rdfs:label "is discovered by organization"@en .


###  https://w3id.org/skgo/modsci#isDiscoveredByScience
:isDiscoveredByScience rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :isDiscoveredBy ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( <http://sweetontology.net/phen/Phenomenon>
                                                   :ScientificDiscovery
                                                 )
                                   ] ;
                       rdfs:range :ModernScience ;
                       rdfs:comment "A phenomenon or a scientific discovery is discovered by science."@en ;
                       rdfs:label "is discovered by science"@en .


###  https://w3id.org/skgo/modsci#isDiscoveredByScientist
:isDiscoveredByScientist rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :isDiscoveredBy ;
                         rdfs:comment "A phenomenon or a scientific discovery is discovered by a scientist."@en ;
                         rdfs:label "is discovered by scientist"@en .


###  https://w3id.org/skgo/modsci#isInterestingToScientist
:isInterestingToScientist rdf:type owl:ObjectProperty ;
                          rdfs:comment "A phenomenon is interesting to a scientist."@en ;
                          rdfs:label "is Interesting to scientist"@en .


###  https://w3id.org/skgo/modsci#isManufacturerOf
:isManufacturerOf rdf:type owl:ObjectProperty ;
                  rdfs:comment "A manufacturer of a scientific instrument."@en ;
                  rdfs:label "is manufacturer of"@en .


###  https://w3id.org/skgo/modsci#isStudentOf
:isStudentOf rdf:type owl:ObjectProperty ;
             rdfs:domain :Scientist ;
             rdfs:range :Scientist ;
             rdfs:comment "A scientist is a student of another scientist."@en ;
             rdfs:label "is student of"@en .


