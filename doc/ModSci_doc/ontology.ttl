###  https://w3id.org/skgo/modsci#appliesLawsFrom
:appliesLawsFrom rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ;
                 rdfs:domain :ModernScience ;
                 rdfs:range :ModernScience ;
                 rdfs:comment "Science applies lows from anthoer one."@en ;
                 rdfs:label "applies laws from"@en .


###  https://w3id.org/skgo/modsci#appliesLawsFromMathematics
:appliesLawsFromMathematics rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :appliesLawsFrom ;
                            rdfs:comment "Science applies laws from Mathematics."@en ;
                            rdfs:label "applies laws from Mathematics"@en .


###  https://w3id.org/skgo/modsci#appliesLawsFromPhysics
:appliesLawsFromPhysics rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :appliesLawsFrom ;
                        rdfs:comment "Science applies laws from Physics."@en ;
                        rdfs:label "applies laws from Physics"@en .


###  https://w3id.org/skgo/modsci#appliesLawsFromStatistics
:appliesLawsFromStatistics rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf :appliesLawsFrom ;
                           rdfs:comment "Science applies laws from Statistics."@en ;
                           rdfs:label "applies laws from Statistics"@en .


###  https://w3id.org/skgo/modsci#appliesLawsFromThermodynamics
:appliesLawsFromThermodynamics rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf :appliesLawsFromPhysics ;
                               rdfs:comment "Science applies laws from Thermodynamics."@en ;
                               rdfs:label "applies laws from Thermodynamics"@en .


###  https://w3id.org/skgo/modsci#belongsToScience
:belongsToScience rdf:type owl:ObjectProperty ;
                  owl:inverseOf :hasScientist ;
                  rdfs:domain :Scientist ;
                  rdfs:range :ModernScience ;
                  rdfs:comment "A scientist belongs to a science."@en ;
                  rdfs:label "belongs to science"@en .


###  https://w3id.org/skgo/modsci#discoveredByAppliedSciences
:discoveredByAppliedSciences rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :isDiscoveredByScience ;
                             rdfs:label "discovered by applied science"@en .


###  https://w3id.org/skgo/modsci#discoveredByBehaviouralSciences
:discoveredByBehaviouralSciences rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf :isDiscoveredByScience ;
                                 rdfs:label "discovered by behavioural science"@en .


###  https://w3id.org/skgo/modsci#discoveredByFormalScience
:discoveredByFormalScience rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf :isDiscoveredByScience ;
                           rdfs:label "discovered by formal science"@en .


###  https://w3id.org/skgo/modsci#discoveredByHealthSciences
:discoveredByHealthSciences rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :isDiscoveredByScience ;
                            rdfs:label "discovered by health science"@en .


###  https://w3id.org/skgo/modsci#discoveredByInterdisciplinaryStudies
:discoveredByInterdisciplinaryStudies rdf:type owl:ObjectProperty ;
                                      rdfs:subPropertyOf :isDiscoveredByScience ;
                                      rdfs:label "discovered by Interdisciplinary studies"@en .


###  https://w3id.org/skgo/modsci#discoveredByNaturalScience
:discoveredByNaturalScience rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :isDiscoveredByScience ;
                            rdfs:label "discovered by natural science"@en .


###  https://w3id.org/skgo/modsci#discoveredBySocialSciences
:discoveredBySocialSciences rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :isDiscoveredByScience ;
                            rdfs:label "discovered by social science"@en .


###  https://w3id.org/skgo/modsci#discovers
:discovers rdf:type owl:ObjectProperty ;
           owl:inverseOf :isDiscoveredBy ;
           rdfs:comment "A scientist or an organization discovers a phenomenon."@en ;
           rdfs:label "discovers"@en .


###  https://w3id.org/skgo/modsci#followedByScientist
:followedByScientist rdf:type owl:ObjectProperty ;
                     owl:inverseOf :followsMethod ;
                     rdfs:comment "A scientific method is followed by a scientist."@en ;
                     rdfs:label "followed by scientist"@en .


###  https://w3id.org/skgo/modsci#followsMethod
:followsMethod rdf:type owl:ObjectProperty ;
               rdfs:domain :Scientist ;
               rdfs:range :ScientificMethod ;
               rdfs:comment "A scientist follows a scientific method."@en ;
               rdfs:label "follows method"@en .


###  https://w3id.org/skgo/modsci#hasApplication
:hasApplication rdf:type owl:ObjectProperty ;
                owl:inverseOf :isApplicationOfScience ;
                rdfs:domain :ModernScience ;
                rdfs:range :ApplicationOfScience ;
                rdfs:comment "A science has an application."@en ;
                rdfs:label "has application"@en .


###  https://w3id.org/skgo/modsci#hasCloseRelationshipTo
:hasCloseRelationshipTo rdf:type owl:ObjectProperty ,
                                 owl:SymmetricProperty ,
                                 owl:ReflexiveProperty ;
                        rdfs:domain :ModernScience ;
                        rdfs:range :ModernScience ;
                        rdfs:comment "A science has a close relationship to another one."@en ;
                        rdfs:label "has close relationship to"@en .


###  https://w3id.org/skgo/modsci#hasCollaborationWith
:hasCollaborationWith rdf:type owl:ObjectProperty ,
                               owl:SymmetricProperty ,
                               owl:ReflexiveProperty ;
                      rdfs:domain :ModernScience ;
                      rdfs:range :ModernScience ;
                      rdfs:comment "Science has a close collaboration with another one."@en ;
                      rdfs:label "has collaboration with"@en .


###  https://w3id.org/skgo/modsci#hasManufacturer
:hasManufacturer rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isManufacturerOf ;
                 rdfs:domain :ScientificInstrument ;
                 rdfs:range :ScientificInstrumentManufacturer ;
                 rdfs:comment "A scientific instrument has a particular manufacturer."@en ;
                 rdfs:label "has manufacturer"@en .


###  https://w3id.org/skgo/modsci#hasMethodsInspiredFrom
:hasMethodsInspiredFrom rdf:type owl:ObjectProperty ;
                        rdfs:domain :ModernScience ;
                        rdfs:range :ModernScience ;
                        rdfs:comment "Science has methods inspired from another science."@en ;
                        rdfs:label "has methods inspired from"@en .


###  https://w3id.org/skgo/modsci#hasResearchBy
:hasResearchBy rdf:type owl:ObjectProperty ;
               owl:inverseOf :undertakesResearch ;
               rdfs:comment "A science has research done by a scientist."@en ;
               rdfs:label "has research by"@en .


###  https://w3id.org/skgo/modsci#hasRoleIn
:hasRoleIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Scientist ;
           rdfs:range :ScientificDiscovery ;
           rdfs:comment "A scientist has a role in a scientific discovery."@en ;
           rdfs:label "has role in"@en .


###  https://w3id.org/skgo/modsci#hasScientificInstrumentManufacturer
:hasScientificInstrumentManufacturer rdf:type owl:ObjectProperty ;
                                     rdfs:subPropertyOf :hasManufacturer ;
                                     rdfs:comment "A scientific instrument has a scientific instrument manufacturer."@en ;
                                     rdfs:label "has scientific instrument manufacturer"@en .


###  https://w3id.org/skgo/modsci#hasScientist
:hasScientist rdf:type owl:ObjectProperty ;
              rdfs:comment "Science has a scientist that it belongs to."@en ;
              rdfs:label "has scientist"@en .


###  https://w3id.org/skgo/modsci#helpInDiscoveryOfPhenomenon
:helpInDiscoveryOfPhenomenon rdf:type owl:ObjectProperty ,
                                      owl:FunctionalProperty ;
                             rdfs:domain :helpInDiscovery ;
                             rdfs:range [ rdf:type owl:Restriction ;
                                          owl:onProperty :helpInDiscoveryOfPhenomenon ;
                                          owl:allValuesFrom <http://sweetontology.net/phen/Phenomenon>
                                        ] ;
                             rdfs:comment "This relation represents that one or more sciences helped in the discovery of the phenomenon."@en ;
                             rdfs:label "help in discovery of phenomenon"@en .


###  https://w3id.org/skgo/modsci#inspiredBy
:inspiredBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :inspiresMethod ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :ScientificMethod ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( <http://sweetontology.net/phen/Phenomenon>
                                       :ScientificDiscovery
                                     )
                       ] ;
            rdfs:comment "A scientific method is inspired by a phenomenon or a scientific discovery."@en ;
            rdfs:label "inspired by"@en .


###  https://w3id.org/skgo/modsci#inspiredFromPhenomenon
:inspiredFromPhenomenon rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :inspiredBy ;
                        rdfs:comment "A scientific method is inspired by a phenomenon."@en ;
                        rdfs:label "inspired by phenomenon"@en .


###  https://w3id.org/skgo/modsci#inspiredFromScientificDiscovery
:inspiredFromScientificDiscovery rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf :inspiredBy ;
                                 rdfs:comment "A scientific method is inspired by a scientific discovery."@en ;
                                 rdfs:label "inspired by scientific discovery"@en .


###  https://w3id.org/skgo/modsci#inspiresMethod
:inspiresMethod rdf:type owl:ObjectProperty ;
                rdfs:comment "A phenomenon or a scientific discovery inspires a scientific method."@en ;
                rdfs:label "inspires method"@en .


###  https://w3id.org/skgo/modsci#instrumentInventedBy
:instrumentInventedBy rdf:type owl:ObjectProperty ;
                      owl:inverseOf :inventsInstrument ;
                      rdfs:domain :ScientificInstrument ;
                      rdfs:range :Scientist ;
                      rdfs:comment "An instrument is invented by a sciientist."@en ;
                      rdfs:label "instrument invented by"@en .


###  https://w3id.org/skgo/modsci#instrumentUsedInScience
:instrumentUsedInScience rdf:type owl:ObjectProperty ;
                         owl:inverseOf :usesInstrument ;
                         rdfs:domain :ScientificInstrument ;
                         rdfs:range :ModernScience ;
                         rdfs:comment "An instrument is used in science."@en ;
                         rdfs:label "instrument used in Science" .


###  https://w3id.org/skgo/modsci#interestedInPhenomenon
:interestedInPhenomenon rdf:type owl:ObjectProperty ;
                        owl:inverseOf :isInterestingToScientist ;
                        rdfs:domain :Scientist ;
                        rdfs:range <http://sweetontology.net/phen/Phenomenon> ;
                        rdfs:comment "A scientist is interested in phenomenon."@en ;
                        rdfs:label "interested in phenomenon"@en .


###  https://w3id.org/skgo/modsci#inventsInstrument
:inventsInstrument rdf:type owl:ObjectProperty ;
                   rdfs:comment "A sciientist invents an instrument ."@en ;
                   rdfs:label "invents instrument"@en .


